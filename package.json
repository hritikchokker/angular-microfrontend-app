{
  "name": "angular-multi-project-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "serve:website": "ng serve website --port 4200",
    "serve:admin": "ng serve website --port 4000",
    "serve:dashboard": "ng serve website --port 4100",
    "build:website": "ng build --project=website",
    "build:admin": "ng build --project=admin",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:project": "ng build --project=website && ng build --project=admin --base-href /admin/ && ng build --project=dashboard --base-href /dashboard/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.5",
    "@angular/cdk": "^12.0.5",
    "@angular/common": "~12.0.5",
    "@angular/compiler": "~12.0.5",
    "@angular/core": "~12.0.5",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.0.5",
    "@angular/material": "^12.0.5",
    "@angular/platform-browser": "~12.0.5",
    "@angular/platform-browser-dynamic": "~12.0.5",
    "@angular/router": "~12.0.5",
    "@angular/service-worker": "~12.0.5",
    "protractor": "^7.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.5",
    "@angular/cli": "~12.0.5",
    "@angular/compiler-cli": "~12.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "source-map-explorer": "^2.5.2",
    "typescript": "~4.2.3"
  }
}
